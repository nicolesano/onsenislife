<h1>Add an onsen</h1>

<%= simple_form_for [@onsen] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :rating %>
  <%= f.input :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Add your onsen", class: "btn btn-primary" %>
<% end %>
